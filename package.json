  },
  "devDependencies": {
    "@jest/globals": "^29.6.2",
    "@types/jest": "^29.5.3",
    "jest": "^29.6.2",
    "prettier": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  }
}
